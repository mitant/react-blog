{
  "name": "article",
  "plural": "articles",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "created": {
      "type": "date",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "blurb": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "string"
    },
    "featurable": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "featuredImage": {
      "type": "string"
    },
    "tags": {
      "type": ["array"]
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
